#importing
import cv2
import numpy as np

#Reading an  image 
image = cv2.imread('C:/Users/91720/Downloads/daylight-environment-forest-459225.png')

#Converting Image to Matrix
image_matrix = np.array(image)

#Displaying output
print(image_matrix)
cv2.imshow("Image",image)
cv2.waitKey(0)
cv2.destroyAllWindows()             
